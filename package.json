{
  "name": "usersphere-cli",
  "version": "1.0.0",
  "description": "A RAG-based CLI tool for user and device management using local embedding models",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "usersphere": "dist/index.js"
  },
  "homepage": "https://github.com/HeiSir2014/UserSphere#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeiSir2014/UserSphere.git"
  },
  "bugs": {
    "url": "https://github.com/HeiSir2014/UserSphere/issues"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run node-ts -- src/index.ts",
    "start:gemma": "npm run node-ts -- src/index.ts --model ./models/embeddinggemma-300M-Q8_0.gguf",
    "start:gemma:q4": "npm run node-ts -- src/index.ts --model ./models/embeddinggemma-300m-Q4_0.gguf",
    "start:build": "npm run build && node dist/index.js",
    "dev": "node --loader ts-node/esm --watch src/index.ts",
    "test": "npm run build && node scripts/test-functionality.js",
    "test:real": "npm run build && node scripts/test-real-model.js",
    "test:real:debug": "npm run build && node --inspect-brk scripts/test-real-model.js",
    "test:similarity": "npm run build && node scripts/debug-similarity.js",
    "debug:quality": "npm run build && node scripts/debug-embedding-quality.js",
    "test:cosine": "npm run build && node scripts/test-cosine-fix.js",
    "test:embedding": "npm run build && node scripts/test-duplicate-embedding.js",
    "example": "npm run build && node --loader ts-node/esm examples/basic-usage.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "setup": "npm run build && npm run test",
    "node-ts": "node --loader ts-node/esm"
  },
  "keywords": [
    "cli",
    "rag",
    "embedding",
    "typescript",
    "node-llama-cpp",
    "faiss",
    "ai",
    "nlp",
    "semantic-search",
    "vector-database",
    "intent-matching",
    "multilingual",
    "qwen",
    "local-ai"
  ],
  "author": {
    "name": "HeiSir2014",
    "email": "heisir21@163.com",
    "url": "https://github.com/HeiSir2014"
  },
  "license": "MIT",
  "dependencies": {
    "faiss-node": "^0.5.1",
    "node-llama-cpp": "^3.13.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/HeiSir2014"
  }
}

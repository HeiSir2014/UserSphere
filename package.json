{
  "name": "usersphere-cli",
  "version": "1.0.0",
  "description": "A RAG-based CLI tool for user and device management using local embedding models",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "usersphere": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "npm run build && node scripts/test-functionality.js",
    "test:real": "npm run build && node scripts/test-real-model.js",
    "test:real:debug": "npm run build && node --inspect-brk scripts/test-real-model.js",
    "test:similarity": "npm run build && node scripts/debug-similarity.js",
    "example": "npm run build && ts-node examples/basic-usage.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "setup": "npm run build && npm run test"
  },
  "keywords": [
    "cli",
    "rag",
    "embedding",
    "typescript",
    "llama",
    "faiss"
  ],
  "author": "HeiSir2014",
  "license": "MIT",
  "dependencies": {
    "faiss-node": "^0.5.1",
    "node-llama-cpp": "^3.13.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
